{
    "name": "laravel-notification-channels/frog-notification-channel",
    "description": "This channel allows you to send notifications via the Frog API provided by Wigal.",
    "homepage": "https://github.com/laravel-notification-channels/frog-notification-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Elikem Z. Seake-Kwawu",
            "email": "skelikem@gmail.com",
            "homepage": "https://github.com/eskayamadeus",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/notifications": "~10.0 || ~11.0",
        "illuminate/support": "~10.0 || ~11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\:channel_namespace\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\:channel_namespace\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
